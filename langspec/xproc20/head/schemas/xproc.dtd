<!ELEMENT %data; (%annotation;)*>
<!ATTLIST %data;
  %global-attrs;
  href %URIref; #REQUIRED
  wrapper %QName; #IMPLIED
  content-type CDATA #IMPLIED>


<!ELEMENT %empty; (%annotation;)*>
<!ATTLIST %empty;
  %global-attrs;>

<!ELEMENT %documentation; (p | div %doc-elts;)*>
<!ATTLIST %documentation;
