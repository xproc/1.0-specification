<!ELEMENT %library; 
   ((%import;) |
    (%declare-step;) |
    (%pipeline;) |
    (%annotation;))*>
<!ATTLIST %library;
  %global-attrs;
  %top-attrs;
  %nds; %URIref; #IMPLIED>

<!ELEMENT %import; (%annotation;)*>
<!ATTLIST %import;
  %global-attrs;
  href %URIref; #REQUIRED
